<?xml version="1.0" encoding="utf-8" ?>

<configuration>
  <configSections>
    <sectionGroup name="applicationSettings" type="System.Configuration.ApplicationSettingsGroup, System, Version=2.0.0.0, Culture=neutral, PublicKeyToken=b77a5c561934e089">
       <!-- KZE: Ensure the following entry exists in your config file (copy if required) -->
      <section name="Intergraph.IPS.Germany.Logging.Properties.Settings" type="System.Configuration.ClientSettingsSection, System, Version=4.0.0.0, Culture=neutral, PublicKeyToken=b77a5c561934e089" requirePermission="false"/>
    </sectionGroup>
  </configSections>

  <applicationSettings>
    <!-- KZE:
      Settings Info:
      - for LogPath, LogFileName, LogFileExtension: empty "" or "*" = use defaults
      - for numeric values: -1 = use defaults
      - for EncodingType: 0=DEFAULT, 1=UTF8, 2=UTF16(LE)    
      Recommended use:
      - Add only settings to your application config file that should NOT have the default value!
      All settings in this template file have default values!
      Do not forget to ensure that the following section is listed in sectionGroup above!
      
      If the following block does not yet exist, copy it into your config file.
      Put it inside application settings.
      - but keep only intended and required settings!
    -->
    <Intergraph.IPS.Germany.Logging.Properties.Settings>
      <setting name="LogPath" serializeAs="String">
        <value>*</value>
      </setting>
      <setting name="LogFileName" serializeAs="String">
        <value>*</value>
      </setting>
      <setting name="LogFileExtension" serializeAs="String">
        <value>*</value>
      </setting>
      <setting name="DebugLevel" serializeAs="String">
        <value>-1</value>
      </setting>
      <setting name="EncodingType" serializeAs="String">
        <value>-1</value>
      </setting>
      <setting name="FileNum1Mode" serializeAs="String">
        <value>-1</value>
      </setting>
      <setting name="ContextMode" serializeAs="String">
        <value>-1</value>
      </setting>
      <setting name="NetworkMode" serializeAs="String">
        <value>-1</value>
      </setting>
      <setting name="ModuleMode" serializeAs="String">
        <value>-1</value>
      </setting>
      <setting name="PurgeMode" serializeAs="String">
        <value>-1</value>
      </setting>
      <setting name="PurgeDaysToKeep" serializeAs="String">
        <value>-1</value>
      </setting>
      <setting name="MaxFileSize_MB" serializeAs="String">
        <value>-1</value>
      </setting>
      <setting name="MinFileFree_KB" serializeAs="String">
        <value>-1</value>
      </setting>
      <setting name="FallbackRecoverInterval_min" serializeAs="String">
        <value>-1</value>
      </setting>
    </Intergraph.IPS.Germany.Logging.Properties.Settings>
  </applicationSettings>

  <!-- KZE: This should be already in your config file, if not add this! -->
  <startup>
    <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.0"/>
  </startup>
  
</configuration>
